<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".patrol"
    android:background="@drawable/biru12">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@drawable/app_icon" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="match_parent"
            android:layout_height="82dp"
            android:background="@drawable/header"
            android:gravity="center"
            android:text="Data Profil Anda"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="match_parent"
            android:layout_height="15dp"

            android:layout_marginTop="10dp"
            android:text="Nama :"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/editText17"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/teks3"
            android:ems="10"
            android:hint="Masukkan nama"
            android:inputType="textPersonName"
            android:textColor="#000000"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="match_parent"
            android:layout_height="15dp"

            android:layout_marginTop="10dp"
            android:text="Nim :"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/editText18"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/teks3"
            android:ems="10"
            android:hint="Masukkan Nim"
            android:inputType="textPersonName"
            android:textColor="#000000"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_marginTop="10dp"
            android:text="Tanggal Lahir :"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/editText19"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/teks3"
            android:ems="10"
            android:hint="Masukkan tanggal lahir"
            android:inputType="textPersonName"
            android:textColor="#000000"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_marginTop="10dp"
            android:text="Tempat Lahir :"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/editText20"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/teks3"
            android:ems="10"
            android:hint="Masukkan Tempat Lahir"
            android:inputType="textPersonName"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_marginTop="10dp"
            android:text="Pembimbing :"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/editText21"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/teks3"
            android:ems="10"
            android:hint="Pembimbing.."
            android:inputType="textPersonName"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_marginTop="10dp"
            android:text="Alamat :"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/editText22"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/teks3"
            android:ems="10"
            android:gravity="start"
            android:hint="Alamat Anda.."
            android:inputType="textPersonName"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_marginTop="10dp"
            android:text="Kelas :"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/editText23"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/teks3"
            android:ems="10"
            android:hint="Kelas"
            android:inputType="textPersonName"
            android:textSize="15dp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="140dp"
            android:background="@drawable/tombol2"
            android:text="Kembali"
            android:textColor="#ffffff"
            android:textSize="10dp" />


    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>