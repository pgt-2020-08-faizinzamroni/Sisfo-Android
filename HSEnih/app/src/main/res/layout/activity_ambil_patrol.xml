<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ambil_patrol"
    android:orientation="vertical"
    android:background="@drawable/biru12">
    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_gravity="center_horizontal"
        app:srcCompat="@drawable/app_icon" />
    <TextView
        android:id="@+id/textView35"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="KHS"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:background="@drawable/header"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/textView43"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:text="Mata Kuliah :"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:textSize="10dp"
        android:textColor="#000000"/>

    <EditText
        android:id="@+id/editText31"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:ems="10"
        android:background="@drawable/teks3"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:hint="Masukkan Mata Kuliah"
        android:inputType="textPersonName"
        android:textSize="15dp"
        android:textColor="#000000"
        />
    <TextView
        android:id="@+id/textView44"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:layout_marginLeft="10dp"
        android:text="Semester :"
        android:layout_marginTop="10dp"
        android:textColor="#000000"
        android:textSize="10dp"/>

    <EditText
        android:id="@+id/editText32"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:ems="10"
        android:textSize="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/teks3"
        android:inputType="textPersonName"
        android:hint="Semester" />

    <Button
        android:id="@+id/button8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="Cari"
        android:textSize="10dp"
        android:textColor="#ffffff"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="140dp"
        android:layout_marginRight="140dp"
        android:background="@drawable/tombol2"
        />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:text="Nama Mata Kuliah :"
        android:textColor="#000000"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/editText24"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@drawable/teks3"
        android:ems="10"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:hint="Nama Mata Kuliah"
        android:inputType="textPersonName"
        android:textSize="15dp"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/textView37"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:text="Sks :"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:textSize="10dp"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/editText25"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:ems="10"
        android:background="@drawable/teks3"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:hint="Sks"
        android:inputType="textPersonName"
        android:textSize="15dp"
        android:textColor="#000000"
        />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:text="Nilai :"
        android:textSize="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:textColor="#000000"
        />

    <TextView
        android:id="@+id/editText26"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:ems="10"
        android:textSize="15dp"
        android:background="@drawable/teks3"
        android:textColor="#000000"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:inputType="textPersonName"
        android:hint="Nilai" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:text="Kode Mata Kuliah :"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:textColor="#000000"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/editText27"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:ems="10"
        android:background="@drawable/teks3"
        android:inputType="textPersonName"
        android:hint="Kode Mata Kuliah"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:textSize="15dp"/>

    <Button
        android:id="@+id/button7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="Kembali"
        android:textSize="10dp"
        android:textColor="#ffffff"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="140dp"
        android:layout_marginRight="140dp"
        android:background="@drawable/tombol2"
        />


</LinearLayout>